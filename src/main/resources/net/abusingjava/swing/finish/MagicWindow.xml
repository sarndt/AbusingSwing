<window
	min-width="400"
	min-height="200"
	
	width="650"
	height="350">

	<panel>
	
		<hbox>
			<vbox background="red" />
			<vbox background="blue" />
			<vbox width="200px">
				<textfield width="50%"></textfield>
				<textfield width="3*">Value</textfield>
				<textfield width="2*" default-text="Click me..."></textfield>
				<button>Button</button>
				<button enabled="false">Button</button>
				<checkbox>Checkbox 1</checkbox>
				<hbox height="27px">
					<checkbox width="27px" /><button>Lebenslauf</button>
				</hbox>
				<checkbox selected="true">Checkbox 2</checkbox>
				<busy-label busy="true" delay="2"></busy-label>
				
				<vbox />
			</vbox>
			
			<vbox width="200px">
			
				<progressbar max="100" value="37" string-painted="true" string="%s%%" />
			
				<hbox height="100px">
					<progressbar max="100" value="10" />
					<progressbar max="100" value="20" />
					<progressbar max="100" value="25" />
					<progressbar max="100" value="60" />
				</hbox>
			
			</vbox>
			
			<vbox>
				
			</vbox>
		</hbox>
	
	</panel>

</window>