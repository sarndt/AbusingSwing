<window
	min-width="400"
	min-height="200"
	
	width="650"
	height="350">

	<panel>
		 
		<hbox>
			<vbox background="red" />
			<vbox background="blue" />
			<vbox width="200px">
				<textfield width="50%"></textfield>
				<textfield width="3*">Value</textfield>
				<textfield width="2*" default-text="Click me..."></textfield>
				<button>Button</button>
				<button enabled="false">Button</button>
				<checkbox>Checkbox 1</checkbox>
				<hbox height="27px">
					<checkbox width="27px" /><button>Lebenslauf</button>
				</hbox>
				<checkbox selected="true">Checkbox 2</checkbox>
				<busy-label busy="true" delay="2"></busy-label>
				
				<vbox />
			</vbox>
			
			<vbox width="200px">
			
				<progressbar max="100" value="37" string-painted="true" string="%s%%" />
			
				<hbox height="100px">
					<progressbar max="100" value="10" />
					<progressbar max="100" value="20" />
					<progressbar max="100" value="25" />
					<progressbar max="100" value="60" />
				</hbox>
			
				<box height="200px">
					
					<box x="0px" y="0px" width="100px" height="100px" background="red" />
					<box x="100px" y="0px" width="100px" height="100px" background="blue" />
					<box x="0px" y="100px" width="100px" height="100px" background="green" />
					<box x="100px" y="100px" width="100px" height="100px" background="yellow" />
					
				</box>
			
			</vbox>
			
			<box background="black">
				
				<box x="50%" y="50%" width="100px" height="200px" background="white" />
				
			</box>
		</hbox>
	
	</panel>

</window>